<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <Authors>Akash Kava</Authors>
    <Company>NeuroSpeech Technologies Pvt Ltd</Company>
    <Product>Mock for EFCore with LocalDB</Product>
    <PackageProjectUrl>https://github.com/neurospeech/ef-core-localdb-mock</PackageProjectUrl>
    <RepositoryUrl>https://github.com/neurospeech/ef-core-localdb-mock</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>EF Core, LocalDB, Mock</PackageTags>
    <Copyright>2017 NeuroSpeech</Copyright>
    <PackageLicenseUrl>https://github.com/neurospeech/ef-core-localdb-mock/blob/master/LICENSE</PackageLicenseUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="2.1.3" />
    <PackageReference Include="NeuroSpeech.EFCoreLiveMigration" Version="1.0.22" />
  </ItemGroup>

</Project>
